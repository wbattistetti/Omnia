{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "be:express": "node backend/server.js",
    "be:api": "set EXPRESS_BASE=http://localhost:3100&& ./.venv/Scripts/python.exe -m uvicorn backend.groq_ddt_api:app --host 127.0.0.1 --port 8000 --reload",
    "be:apiNew": "uvicorn newBackend.app:app --host 127.0.0.1 --port 8000 --reload",
    "dev:beOld": "concurrently -n express,api -c yellow,cyan \"npm:be:express\" \"npm:be:api\"",
    "dev:beNew": "concurrently -n express,apiNew -c yellow,cyan \"npm:be:express\" \"npm:be:apiNew\"",
    "dev:all": "concurrently -n web,express,api -c green,yellow,cyan \"vite\" \"npm:be:express\" \"npm:be:api\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/uuid": "^10.0.0",
    "compromise": "^14.14.4",
    "compromise-dates": "^3.7.1",
    "dockview": "^4.5.0",
    "fuse.js": "^7.1.0",
    "libphonenumber-js": "^1.12.12",
    "lucide-react": "^0.344.0",
    "monaco-editor": "^0.52.2",
    "mongodb": "^6.17.0",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dockable": "^0.0.1",
    "react-dom": "^18.3.1",
    "react-monaco-editor": "^0.59.0",
    "react-popper": "^2.3.0",
    "reactflow": "^11.11.4",
    "uuid": "^11.1.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
