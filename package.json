{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "redis:up": "docker-compose up -d redis || echo Redis startup failed (Docker may not be running)",
    "redis:up:ignore": "docker-compose up -d redis 2>&1 || exit 0",
    "redis:down": "docker-compose down redis",
    "redis:check": "docker ps --filter name=redis --format \"{{.Status}}\"",
    "be:express": "node --watch backend/server.js",
    "be:api": "set EXPRESS_BASE=http://localhost:3100&& ./.venv/Scripts/python.exe -m uvicorn backend.groq_ddt_api:app --host 127.0.0.1 --port 8000 --reload",
    "be:apiNew": "node backend/scripts/startPythonService.js",
    "be:apiNew:direct": "uvicorn newBackend.app:app --host 127.0.0.1 --port 8000 --reload",
    "be:ruby": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Set-Location 'backend/ruby'; ./start-ruby.bat\"",
    "be:vbnet": "cd VBNET/ApiServer && dotnet run",
    "dev:beOld": "npm run redis:up:ignore && concurrently -n express,api,ruby -c yellow,cyan,magenta \"npm:be:express\" \"npm:be:api\" \"npm:be:ruby\"",
    "dev:beNew": "npm run redis:up:ignore && concurrently -n express,apiNew,ruby -c yellow,cyan,magenta \"npm:be:express\" \"npm:be:apiNew\" \"npm:be:ruby\"",
    "dev:all": "npm run redis:up && concurrently -n web,express,api,ruby -c green,yellow,cyan,magenta \"vite\" \"npm:be:express\" \"npm:be:api\" \"npm:be:ruby\"",
    "dev:allNew": "npm run redis:up && concurrently -n web,express,apiNew,ruby -c green,yellow,cyan,magenta \"vite\" \"npm:be:express\" \"npm:be:apiNew\" \"npm:be:ruby\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:wizard": "vitest run src/wizard/__tests__",
    "test:integration": "node scripts/test-wizard-integration.js",
    "seed:act-patterns": "node scripts/seed-act-type-patterns.js",
    "migrate:translations": "node backend/scripts/migrateTranslationsToBCP47.js",
    "migrate:translations:dry-run": "node backend/scripts/migrateTranslationsToBCP47.js --dry-run",
    "verify:domain-purity": "node scripts/verify-domain-purity.js",
    "verify:imports": "node scripts/verify-imports.js",
    "verify:features": "node scripts/verify-features.js",
    "verify:no-fallbacks": "node scripts/verify-no-fallbacks.js",
    "verify:architecture": "npm run verify:domain-purity && npm run verify:imports && npm run verify:features && npm run verify:no-fallbacks"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@types/uuid": "^10.0.0",
    "compromise": "^14.14.4",
    "compromise-dates": "^3.7.1",
    "dockview": "^4.5.0",
    "fuse.js": "^7.1.0",
    "libphonenumber-js": "^1.12.12",
    "lucide-react": "^0.344.0",
    "monaco-editor": "^0.52.2",
    "mongodb": "^6.17.0",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dockable": "^0.0.1",
    "react-dom": "^18.3.1",
    "react-monaco-editor": "^0.59.0",
    "react-popper": "^2.3.0",
    "react-select": "^5.10.2",
    "reactflow": "^11.11.4",
    "redis": "^4.6.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winreg": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^3.2.4"
  }
}